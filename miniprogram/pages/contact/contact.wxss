/* miniprogram/pages/contact/contact.wxss */
.contact{
  width:100%;
  height:60rpx;
  border:1px solid rgba(0, 0, 0, 0.1);
  text-align: center;
  line-height: 60rpx;
  font-weight: 500;
  font-size:40rpx;
}
.list-item{
  width:100%;
  height:180rpx;
  margin-bottom:20rpx;
  padding-left:30rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  border:1px solid rgba(0, 0, 0, 0.1);
  background-color:aliceblue;
}
.list-item .list-img{
  width:150rpx;
  height:150rpx;
  margin:25rpx;
}
.list-item text{
  font-size:30rpx;
}
.list-text{
  width:500rpx;
  margin-left:10rpx;
}

.nav-list{
  width:700rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}