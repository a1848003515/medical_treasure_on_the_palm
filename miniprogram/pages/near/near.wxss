/**index.wxss**/

.items {
  float: left;
  width: 20%;
  background-color: #fff;
  margin-left:20%;
}

.item-img {
  width: 100%;
  height: 50rpx;
  margin-top: 10rpx;
}

.item-text {
  width: 100%;
  height: 50rpx;
  font-size: 25rpx;
  line-height: 50rpx;
  text-align: center;
}
.data{
  height:700rpx;
  width: 99.5%;
  border:1px solid rgba(0, 0, 0, 0.1);
}
.map_container {
  height: 700rpx;
  width: 100%;
}

.map {
  height: 100%;
  width: 100%;
}
.margin-indexbottom{
  padding-bottom:150rpx;
}

.near-list{
  width:100%;
}
.list-item{
  width:100%;
  margin:50rpx 0;
  display: flex;
  align-items: center;
  justify-content: space-between;
  border:1px solid rgba(0, 0, 0, 0.1);
  background-color:aliceblue;
}
.list-item .list-img{
  width:150rpx;
  height:150rpx;
  margin:25rpx;
}
.list-item text{
  font-size:20rpx;
}
.list-text{
  width:420rpx;
}
.list-nav{
  width:150rpx;
  height:150rpx;
  text-align: center;
}
.list-nav image{
  width:80rpx;
  height:80rpx;
}
.map-inputtips-input{
  height: 80rpx;
  line-height: 80rpx;
  width: 100%;
  box-sizing: border-box;
  font-size: 30rpx;
  padding: 0 10px;
  background-color: #fff;
  border-bottom:1px solid #c3c3c3;
}
.map-inputtips-input input{
  border: 1px solid #ddd;
  border-radius: 5px;
  height: 60rpx;
  line-height: 60rpx;
  width: 100%;
  box-sizing: border-box;
  padding: 0 5px;
  margin-top: 10rpx;
}
.nav-list{
  width:700rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
