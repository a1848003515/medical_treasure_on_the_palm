/* miniprogram/pages/index/search/search.wxss */
.search-nav{
  width:100%;
  height:80rpx;
  display: flex;
  justify-content: space-between;
  align-items: center
}
.search-nav input{
  width:65%;
  height:80%;
  border:1px solid black;
  margin-left:20rpx;
  border-radius: 40rpx;
  padding-left:25rpx;
}
.search-nav button:not([size='mini']){
  width:20%;
  height:80%;
  text-align: center;
  line-height: 64rpx;
  font-weight:600
}
.list-item{
  width:100%;
  margin:20rpx 0;
  padding-left:30rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  border:1px solid rgba(0, 0, 0, 0.1);
  background-color:aliceblue;
}
.list-item .list-img{
  width:150rpx;
  height:150rpx;
  margin:25rpx;
}
.list-item text{
  font-size:20rpx;
}
.list-text{
  width:500rpx;
  margin-left:10rpx;
}

.nav-list{
  width:700rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.pharmacy{
  display: flex;
  justify-content: space-between
}
.picker{
  font-size:25rpx;
}
.pharmacy picker{
  width:120rpx;
  text-align: center;
  height:42rpx;
  line-height: 42rpx;
  border:1px solid black;
  border-radius: 40rpx;
}
.list-nav{
  width:150rpx;
  height:150rpx;
  text-align: center;
  margin-right:30rpx;
}
.list-nav image{
  width:80rpx;
  height:80rpx;
}